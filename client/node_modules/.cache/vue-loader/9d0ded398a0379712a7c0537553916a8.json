/BookingService.js'\nexport default {\n  name: \"bookings-form\",\n  data(){\n    return {\n      guest_name: '',\n      email_address: '',\n      checkin_status: null\n    }\n  },\n  methods: {\n    handleFormSubmit(e){\n      e.preventDefault()\n      const payload = {\n        guest_name: this.guest_name,\n        email_address: this.email_address,\n        checkin_status: this.checkin_status\n      }\n      BookingService.postBooking(payload)\n      .then(res => eventBus.$emit('booking-added', res))\n    }\n  }\n}\n</script>\n\n<style lang=\"css\" scoped>\n</style>\n"]}]}